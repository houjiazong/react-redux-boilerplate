{
  "name": "react-redux-boilerplate",
  "version": "0.0.1",
  "description": "React Redux Boilerplate, Starter Kit",
  "main": "server/app.js",
  "scripts": {
    "clean": "rm -rf ./build",
    "predev": "npm run clean",
    "dev": "better-npm-run dev",
    "prebuild": "npm run clean",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node server/app.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "webpack --config ./webpack/webpack.production.config",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "starter-kit"
  ],
  "author": "houjiazong@gmail.com",
  "license": "ISC",
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "axios": "^0.12.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "0.0.8",
    "classnames": "^2.2.5",
    "config": "^1.21.0",
    "consolidate": "^0.14.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "es6-promise": "^3.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "history": "^2.1.2",
    "html-webpack-plugin": "^2.19.0",
    "js-yaml": "^3.6.1",
    "json-loader": "^0.5.4",
    "koa": "^1.2.0",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "koa-views": "^4.1.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "nunjucks": "^2.4.2",
    "react": "^15.1.0",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
