{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "webpack-react-redux-babel-autoprefixer-hmr-postcss-css-modules-eslint-proxy-boilerplate",
  "main": "bin/server",
  "scripts": {
    "clean": "rm -rf ./dist",
    "predev": "npm run clean",
    "prebuild": "npm run clean",
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "server": "better-npm-run server"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon bin/dev",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "node bin/build",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "server": {
      "command": "node bin/server",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/houjiazong/react-redux-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "react",
    "redux",
    "babel",
    "autoprefixer",
    "hmr",
    "postcss",
    "css-modules",
    "react-boilerplate",
    "redux-boilerplate",
    "react-redux-boilerplate"
  ],
  "author": "houjiazong@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/houjiazong/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/houjiazong/react-redux-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "css-loader": "^0.25.0",
    "debug": "^2.2.0",
    "eslint": "^3.9.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-harddisk-plugin": "0.0.2",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^3.11.2",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.1.0",
    "progress": "^1.1.8",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.1"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "classnames": "^2.2.5",
    "config": "^1.24.0",
    "express": "^4.14.0",
    "http-proxy-middleware": "^0.17.2",
    "js-yaml": "^3.7.0",
    "nunjucks": "^3.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
